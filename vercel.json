{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "minihippo.com/**", "use": "@vercel/static" },
    { "src": "cdn.jsdelivr.net/**", "use": "@vercel/static" },
    { "src": "cdnjs.cloudflare.com/**", "use": "@vercel/static" },
    { "src": "code.jquery.com/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/$", "dest": "/minihippo.com/index.html" },
    { "src": "/minihippo\\.com/?$", "dest": "/minihippo.com/index.html" }
  ]
}


